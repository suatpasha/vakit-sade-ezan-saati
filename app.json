{
  "expo": {
    "name": "Vakit",
    "slug": "vakit-sade-ezan-saati",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "locales": {
      "tr": "./locales/tr.json",
      "en": "./locales/en.json"
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suatp.vakitezansaati",
      "buildNumber": "7",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "The app needs your location to accurately calculate prayer times and show the Qibla direction.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "The app needs your location to accurately calculate prayer times and show the Qibla direction."
      }
    },
    "android": {
      "versionCode": 6,
      "useNextNotificationsApi": true,
      "package": "com.suatp.vakitezansaati",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#10B981",
          "sounds": [
            "./assets/sounds/ogle.mp3"
          ],
          "mode": "production",
          "android": {
            "notificationChannelId": "ezan-channel",
            "notificationSound": "ogle.mp3"
          },
          "ios": {
            "sound": true
          }
        }
      ],
      "expo-location",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-6300495112628329~8677481654",
          "iosAppId": "ca-app-pub-6300495112628329~3264954986"
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "87f7aeae-185b-4b61-94df-bb4733892726"
      }
    }
  }
}
